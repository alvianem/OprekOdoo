<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="draft" model="project.version.state">
        <field name="name">Draft</field>
        <field name="sequence">1</field>
    </record>

    <record id="progress" model="project.version.state">
        <field name="name">Work In Progress</field>
        <field name="sequence">2</field>
    </record>
    <record id="staging" model="project.version.state">
        <field name="name">Staging</field>
        <field name="sequence">3</field>
        <field name="done">True</field>
    </record>

    <record id="production" model="project.version.state">
        <field name="name">Production</field>
        <field name="sequence">4</field>
        <field name="done">True</field>
    </record>

    <record id="past" model="project.version.state">
        <field name="name">Past</field>
        <field name="sequence">5</field>
        <field name="done">True</field>
    </record>
</odoo>
